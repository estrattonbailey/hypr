{
  "name": "rola",
  "version": "0.2.10",
  "description": "",
  "main": "dist/rola.js",
  "files": [
    "dist",
    "util",
    "cli.js"
  ],
  "bin": {
    "rola": "./cli.js"
  },
  "scripts": {
    "build": "npm run build:index && npm run build:internals",
    "build:index": "microbundle build -i index.js -o dist -f cjs --target web --jsx React.createElement",
    "build:internals": "microbundle build lib/App.js lib/html.js -o dist -f cjs --jsx React.createElement",
    "watch:index": "microbundle watch -i index.js -o dist -f cjs --target web --jsx React.createElement --compress false",
    "watch:internals": "microbundle watch lib/App.js lib/html.js util/logger.js -o dist -f cjs --jsx React.createElement --compress false",
    "watch": "npm run watch:index & npm run watch:internals"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/estrattonbailey/rola.git"
  },
  "keywords": [
    "spa",
    "single page app",
    "static site generator",
    "react",
    "server side rendering",
    "website",
    "web app"
  ],
  "author": "estrattonbailey",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/estrattonbailey/rola/issues"
  },
  "homepage": "https://github.com/estrattonbailey/rola#readme",
  "dependencies": {
    "@picostate/react": "^1.1.0",
    "@rola/compiler": "^0.2.8",
    "@rola/log": "^0.2.7",
    "@rola/plugin-node": "^0.2.8",
    "@rola/static": "^0.2.10",
    "@rola/util": "^0.2.8",
    "ansi-colors": "^3.2.3",
    "biti": "^2.4.0",
    "commander": "^2.19.0",
    "compression": "^1.7.3",
    "connect": "^3.6.6",
    "deepmerge": "^3.2.0",
    "exit-hook": "^2.0.0",
    "flatted": "^2.0.0",
    "fs-extra": "^5.0.0",
    "html-meta-tags": "^1.1.0",
    "log-update": "^2.3.0",
    "matchit": "^1.0.7",
    "nanoclass": "0.0.2",
    "picostate": "^1.4.0",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "serve-static": "^1.13.2",
    "source-map-support": "^0.5.9",
    "webpack": "^4.29.6"
  },
  "devDependencies": {
    "microbundle": "^0.11.0"
  },
